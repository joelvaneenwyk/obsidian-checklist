{
  "name": "mycoshiro-tasklist",
  "version": "2.2.13",
  "description": "A plugin for Obsidian.md which consolidates todo items into a single view.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "yarn check && node esbuild.config.mjs",
    "check": "tsc -noEmit -skipLibCheck",
    "deploy": "cross-env-shell \"yarn shx --verbose mkdir -p \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" && yarn shx --verbose cp -f styles.css dist/main.js manifest.json versions.json \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" \"",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint \"**/*.ts\" \"**/*.svelte\" ",
    "lint:fix": "eslint --fix \"**/*.ts\" \"**/*.svelte\" ",
    "release": "yarn build && yarn deploy"
  },
  "keywords": [
    "obsidian"
  ],
  "bin": "dist/main.js",
  "author": "Joel Van Eenwyk",
  "authorUrl": "https://github.com/joelvaneenwyk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/mycoshiro-tasklist.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.11",
    "@types/markdown-it": "^13.0.7",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@yarnpkg/sdks": "^3.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-import-glob": "^2.0.0",
    "builtin-modules": "^3.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.10",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "esbuild-svelte": "^0.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json-files": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-regexp": "^2.1.2",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.7.0",
    "markdown-it": "^14.0.0",
    "minimatch": "^9.0.3",
    "obsidian": "^1.4.11",
    "prettier": "^3.1.1",
    "shx": "^0.3.4",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-svelte-plugin": "^0.3.36",
    "unified-lint-rule": "^2.1.2",
    "vue-eslint-parser": "^9.3.2"
  }
}
